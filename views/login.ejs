<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Login - The Simps Quiz</title>

  <!--External CSS-->
  <link href="https://fonts.cdnfonts.com/css/homer-simpson-revised" rel="stylesheet">

  <!--App CSS-->
  <link rel="stylesheet" href="/styles/_nav.css" type="text/css" />
  <link rel="stylesheet" href="/styles/layout.css" type="text/css" />
  <link rel="stylesheet" href="/styles/homepage.css" type="text/css" />

  </head>

  <body>

    <div class="card text-center">
      <div class="card-header"></div>
      <div class="card-body">
        <h5 class="card-title">Simps Quiz Login</h5>

    <form action="/login" method="POST">
      <label for="email">Enter Email</label>
      <input id="email" type="email" name="email">
      <label for="password">Enter Password</label>
      <input id="password" type="password" name="password">
      <input type="submit" value="Submit">

    </form>







  </body>

  </html>
